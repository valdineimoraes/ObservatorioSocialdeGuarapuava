<tr>
  <td><%= meeting.formatted_date %></td>
  <td><%= meeting.start_session.to_time.strftime('%H:%M') %></td>
  <td><%= meeting.end_session.to_time.strftime('%H:%M') %></td>
  <td><%= meeting.note %></td>
  <td><%= link_to meeting.projects.size, meeting_projects_path(meeting) %></td>

   <!--Ações dos Botões -->
  <td><%= link_to '', export_meeting_path(meeting), target: "_blank",
                  class: "fa fa-file-pdf btn btn-success" ,
                  title: "Gerar Relatório em PDF da Sessão"%></td>
  <td><%= link_to '', meeting_path(meeting), class: "btn btn-warning fa fa-search",
                  title: "Visualizar Sessão" %></td>
  <td><%= link_to '', meeting_presents_path(meeting),
                  class: "btn btn-success fa fa-check", title: "Inserir Vereadores Presentes na Sessão" %>
  <td><%= link_to '', edit_meeting_path(meeting.id),
                  class:"btn btn-info fa fa-edit", title: "Editar Sessão"%></td>
  <td><%= link_to '', meeting, method: :delete, data: { confirm: 'Are you sure?' },
                  class:"btn btn-danger fa fa-remove" , title: "Remover Sessão"%></td>
</tr>